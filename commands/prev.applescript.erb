if application "Keynote" is not frontmost then
  tell application "Keynote"
    activate
  end tell
end if

set slideShouldBe to <%= current %>

tell application "Keynote"
  try
    tell front document
      start from slide (slideShouldBe - 1)
    end tell
  on error message
    tell front document
      show previous
    end tell
  end try
  delay 1
  return slide number of current slide of front document
end tell